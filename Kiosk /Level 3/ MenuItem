public class MenuItem {
    private String menu;
    private double price;
    private String detail;

    public MenuItem(String menu, double price, String detail) {
        this.menu = menu;
        this.price = price;
        this.detail = detail;

    }

    public String getMenu() {

        return this.menu;
    }

    public double getPrice() {

        return this.price;
    }

    public String getDetail() {

        return this.detail;
    }



}
