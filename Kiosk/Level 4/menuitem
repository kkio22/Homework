public class MenuItem {

   //변수 생성
    private String name;
    private double price;
    private String detail;



    public MenuItem(String name, double price, String detail) {
        this.name=name;
        this.price=price;
        this.detail=detail;

    }

    public String getName() {
        return this.name; 
    }

    public double getPrice() {
        return this.price;
    }
    public String getDetail(){
        return this.detail;
    }

}
